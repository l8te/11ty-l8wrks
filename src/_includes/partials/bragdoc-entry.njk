<div class="bragdoc__entry">
  <div class="bragdoc__entry-milestone"></div>
  <div class="bragdoc__entry-block">
    <span class="bragdoc__entry-date">
      {{ brag.data.date | readableDate }}
    </span>
    <br/>
    <h2 class="bragdoc__entry-title"><span class="bragdoc__icon">{{ brag.data.icon }}</span> {{ brag.data.title }}</h2>
    <div class="bragdoc__entry-content">
        {{ brag.templateContent | safe }}
    </div>
  </div>
  <div class="bragdoc__taglist">
  {% for category in brag.data.categories %}
    <span># {{category}}</span>
  {% endfor %}
  </div>
</div>