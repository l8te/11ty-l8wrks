<h2>prev + next links</h2>

<p>nav key: {{ eleventyNavigation.key }}</p>
<p>nav parent: {{ eleventyNavigation.parent }}</p>
<p>tags: {{ tags }}</p>


{# get other items with same parent page #}
<p>Previous and Next nav, needs to be fixed</p>
{% if collections.all %}
{% set nextPost = collections.all | getNextCollectionItem(page) %}
{% set previousPost = collections.all | getPreviousCollectionItem(page) %}
{% if nextPost or previousPost %}
<ul>
  {% if nextPost %}<li>Next: <a href="{{ nextPost.url | url }}">{{ nextPost.data.title }}</a></li>{% endif %}
  {% if previousPost %}<li>Previous: <a href="{{ previousPost.url | url }}">{{ previousPost.data.title }}</a></li>{% endif %}
</ul>
{% endif %}
{% endif %} 